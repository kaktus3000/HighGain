@prefix lv2:  <http://lv2plug.in/ns/lv2core#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix doap: <http://usefulinc.com/ns/doap#> .

<https://github.com/kaktus3000/HighGain/AmpI> a lv2:Plugin ;
    doap:name "High Gain Amplifier I" ;
    doap:licence <http://usefulinc.com/doap/licenses/gpl> ;
    lv2:optionalFeature lv2:hardRtCapable ;

    lv2:port [
	a lv2:AudioPort ;
	a lv2:InputPort ;
	lv2:index 0 ;
	lv2:symbol "in" ;
	lv2:name "in" ;
	] , [
	a lv2:AudioPort ;
	a lv2:OutputPort ;
	lv2:index 1 ;
	lv2:symbol "out" ;
	lv2:name "out" ;
	] , [
	a lv2:InputPort ;
	a lv2:ControlPort ;
	lv2:index 2 ;
	lv2:symbol "pre" ;
	lv2:name "Preamp" ;
	lv2:default 0.5 ;
	lv2:minimum 0.0 ;
	lv2:maximum 1.0 ;	
    ] , [
	a lv2:InputPort ;
	a lv2:ControlPort ;
	lv2:index 3 ;
	lv2:symbol "dist" ;
	lv2:name "Dist" ;
	lv2:default 0.5 ;
	lv2:minimum 0.0 ;
	lv2:maximum 1.0 ;	
    ] , [
	a lv2:InputPort ;
	a lv2:ControlPort ;
	lv2:index 4 ;
	lv2:symbol "bass" ;
	lv2:name "Bass" ;
	lv2:default 0.5 ;
	lv2:minimum 0.0 ;
	lv2:maximum 1.0 ;	
    ] , [
	a lv2:InputPort ;
	a lv2:ControlPort ;
	lv2:index 5 ;
	lv2:symbol "mid" ;
	lv2:name "Mid" ;
	lv2:default 0.5 ;
	lv2:minimum 0.0 ;
	lv2:maximum 1.0 ;	
    ] , [
	a lv2:InputPort ;
	a lv2:ControlPort ;
	lv2:index 6 ;
	lv2:symbol "trbl" ;
	lv2:name "Treble" ;
	lv2:default 0.5 ;
	lv2:minimum 0.0 ;
	lv2:maximum 1.0 ;	
    ] , [
	a lv2:InputPort ;
	a lv2:ControlPort ;
	lv2:index 7 ;
	lv2:symbol "vol" ;
	lv2:name "Volume" ;
	lv2:default 0.5 ;
	lv2:minimum 0.0 ;
	lv2:maximum 1.0 ;	
    ] , [
	a lv2:InputPort ;
	a lv2:ControlPort ;
	lv2:index 8 ;
	lv2:symbol "dist_sw" ;
	lv2:name "Dist Switch" ;
	lv2:portProperty lv2:toggled ;
	lv2:default 1.0 ;
	] , [
	a lv2:InputPort ;
	a lv2:ControlPort ;
	lv2:index 9 ;
	lv2:symbol "lead_sw" ;
	lv2:name "Lead Switch" ;
	lv2:portProperty lv2:toggled ;
	lv2:default 0.0 ;
	]
.
